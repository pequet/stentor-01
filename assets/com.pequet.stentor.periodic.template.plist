<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.pequet.stentor.periodic</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/periodic_harvester.sh</string>
        <string>--use-break-on-existing</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>StartCalendarInterval</key>
    <dict>
        <key>Minute</key>
        <integer>0</integer>
    </dict>
    <key>StandardErrorPath</key>
    <string>/tmp/com.pequet.stentor.periodic.err</string>
    <key>StandardOutPath</key>
    <string>/tmp/com.pequet.stentor.periodic.out</string>
</dict>
</plist> 